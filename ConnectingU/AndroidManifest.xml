<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.connectingu"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".UserAccount"
            android:label="@string/title_activity_user_account"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".views.UserPref"
            android:label="@string/title_activity_user_pref"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".SellPage"
            android:label="@string/title_activity_sell_page"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".views.SellActivity"
            android:label="@string/title_activity_sell"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".StartUp"
            android:label="@string/title_activity_start_up"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".StartName"
            android:label="@string/title_activity_start_name" >
        </activity>
        <activity
            android:name=".views.MainMenu"
            android:label="@string/menuMain" >
        </activity>
        <activity
            android:name=".views.Buy"
            android:label="@string/title_activity_buy" >
        </activity>
        <activity
            android:name=".views.Start"
            android:label="@string/title_activity_start" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BookItemView"
            android:label="@string/title_activity_book_item_view" >
        </activity>
    </application>

</manifest>
